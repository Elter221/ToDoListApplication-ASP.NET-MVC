@using ToDoApplicationMVC.BLL.Models
@model ToDoModel


<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Creation Date</th>
            <th>Deadline</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @await Html.PartialAsync("_ToDoRow", Model)
    </tbody>
</table>