@model IEnumerable<ToDoModel>

@section TableHeader {
    <h2>Your ToDo's List</h2>
}

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Creation Date</th>
            <th>Deadline</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            @(await Html.PartialAsync("_ToDoRow", item))
        }
    </tbody>
</table>